{
  "name": "coupdoeil",
  "image": "docker.maple.maceroc.com:5000/millegrilles_coupdoeil_python:2024.0.6",
  "certificat": {
    "roles": ["coupdoeil"],
    "exchanges": ["3.protege", "2.prive", "1.public"],
    "dns": {"localhost": true, "hostnames": ["coupdoeil"], "domain": true}
  },
  "env": {
    "MQ_HOSTNAME": "${MQ_HOSTNAME}",
    "MQ_PORT": "${MQ_PORT}",
    "REDIS_HOSTNAME": "redis",
    "REDIS_PORT": "6379",
    "REDIS_PASSWORD_PATH": "/run/secrets/passwd.redis.txt",
    "CERT_PEM": "/run/secrets/cert.pem",
    "KEY_PEM": "/run/secrets/key.pem",
    "CA_PEM": "/run/secrets/millegrille.cert.pem",
    "WEB_PORT": "1443"
  },
  "constraints": [
    "node.labels.millegrilles.coupdoeil == true"
  ],
  "configs": [
    {
      "name": "pki.coupdoeil",
      "current": "cert",
      "filename": "/run/secrets/cert.pem"
    },
    {
      "name": "pki.millegrille",
      "filename": "/run/secrets/millegrille.cert.pem"
    }
  ],
  "secrets": [
    {
      "name": "pki.coupdoeil",
      "current": "key",
      "filename": "key.pem"
    },
    {
      "name": "passwd.redis",
      "current": "password",
      "filename": "passwd.redis.txt"
    }
  ],
  "networks": [{
    "target": "millegrille_net",
    "aliases": ["coupdoeil"]
  }],
  "labels": {
    "securite": "3.protege",
    "url": "https://${HOSTNAME}/coupdoeil"
  },
  "resources": {
    "cpu_limit": 1000000000,
    "mem_limit": 100000000
  },
  "restart_policy": {
    "condition": "on-failure",
    "delay": 5000000000,
    "max_attempts": 100
  },
  "mode": {
    "mode": "replicated",
    "replicas": 1
  }
}