{
  "name": "coupdoeil",
  "image": "docker.maceroc.com/millegrilles_web_coupdoeil:2023.2.2",
  "certificat": {
    "roles": ["coupdoeil"],
    "exchanges": ["3.protege", "2.prive", "1.public"],
    "dns": {"localhost": true, "hostnames": ["coupdoeil"], "domain": true}
  },
  "env": {
    "MG_MQ_URL": "amqps://mq:5673",
    "MG_REDIS_HOST": "redis",
    "MG_REDIS_PORT": "6379",
    "MG_MQ_CERTFILE": "/run/secrets/cert.pem",
    "MG_MQ_KEYFILE": "/run/secrets/key.pem",
    "MG_MQ_CAFILE": "/run/secrets/millegrille.cert.pem",
    "WEB_CERT": "/run/secrets/cert.pem",
    "WEB_KEY": "/run/secrets/key.pem",
    "SERVER_TYPE": "spdy",
    "HOST": "${HOSTNAME}",
    "PORT": "443"
  },
  "constraints": [
    "node.labels.millegrilles.coupdoeil == true"
  ],
  "configs": [
    {
      "name": "pki.coupdoeil",
      "current": "cert",
      "filename": "/run/secrets/cert.pem"
    },
    {
      "name": "pki.millegrille",
      "filename": "/run/secrets/millegrille.cert.pem"
    }
  ],
  "secrets": [
    {
      "name": "pki.coupdoeil",
      "current": "key",
      "filename": "key.pem"
    },
    {
      "name": "passwd.redis",
      "current": "password",
      "filename": "passwd.redis.txt"
    }
  ],
  "networks": [{
    "target": "millegrille_net",
    "aliases": ["coupdoeil"]
  }],
  "labels": {
    "securite": "3.protege",
    "url": "https://${HOSTNAME}/coupdoeil"
  },
  "resources": {
    "cpu_limit": 1000000000,
    "mem_limit": 250000000
  },
  "restart_policy": {
    "condition": "on-failure",
    "delay": 5000000000,
    "max_attempts": 1
  },
  "mode": {
    "mode": "replicated",
    "replicas": 1
  }
}