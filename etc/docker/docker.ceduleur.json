{
  "name": "ceduleur",
  "image": "docker.maple.maceroc.com:5000/millegrilles_midcompte_python:2024.2.34",
  "args": ["-m", "millegrilles_ceduleur"],
  "certificat": {
    "roles": ["ceduleur"],
    "exchanges": ["4.secure", "3.protege", "2.prive", "1.public"]
  },
  "env": {
    "CA_PEM": "/run/secrets/pki.millegrille.cert",
    "CERT_PEM": "/run/secrets/cert.pem",
    "KEY_PEM": "/run/secrets/key.pem",
    "MQ_HOSTNAME": "${MQ_HOSTNAME}",
    "MQ_PORT": "${MQ_PORT}",
    "REDIS_HOSTNAME": "redis",
    "REDIS_PORT": "6379"
  },
  "constraints": ["node.labels.millegrilles.ceduleur == true"],
  "configs": [
    {
      "name": "pki.millegrille",
      "filename": "/run/secrets/pki.millegrille.cert"
    },
    {
      "name": "pki.ceduleur",
      "current": "cert",
      "filename": "/run/secrets/cert.pem"
    }
  ],
  "secrets": [
    {
      "name": "pki.ceduleur",
      "current": "key",
      "filename": "key.pem"
    }
  ],
  "networks": [{"target": "millegrille_net"}],
  "labels": {
    "millegrille": "${IDMG}"
  },
  "resources": {
    "cpu_limit": 500000000,
    "mem_limit": 250000000
  },
  "restart_policy": {
    "condition": "any",
    "delay": 5000000000,
    "max_attempts": 1000
  },
  "mode": {
    "mode": "replicated",
    "replicas": 1
  }
}
