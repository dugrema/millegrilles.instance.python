{
  "name": "mq",
  "image": "docker.maple.maceroc.com:5000/mg_rabbitmq:3.13-management.master.3",
  "certificat": {"roles": ["mq"], "dns": {"localhost": true, "hostnames": ["mq"], "domain": true}},
  "passwords": ["mqadmin"],
  "env": {
    "MG_KEY": "/run/secrets/key.pem",
    "WEB_KEY": "/run/secrets/webkey.pem",
    "CONFIG_FILE": "/opt/rabbitmq/dist/config/rabbitmq_ipv6management.config",
    "NODENAME": "rabbit@mq"
  },
  "mounts": [{"source": "rabbitmq-data", "target":  "/var/lib/rabbitmq", "type": "volume"}],
  "constraints": ["node.labels.millegrilles.mq == true"],
  "configs": [
    {
      "name": "pki.millegrille",
      "filename": "/opt/rabbitmq/dist/certs/calist.cert.pem"
    },
    {
      "name": "pki.mq",
      "current": "cert",
      "filename": "/opt/rabbitmq/dist/certs/cert.pem"
    },
    {
      "name": "pki.mq",
      "current": "cert",
      "filename": "/opt/rabbitmq/dist/certs/webcert.pem"
    }
  ],
  "secrets": [
    {
      "name": "pki.mq",
      "current": "key",
      "filename": "key.pem"
    },
    {
      "name": "pki.mq",
      "current": "key",
      "filename": "webkey.pem"
    }
  ],
  "endpoint_spec": {
    "mode": "vip",
    "ports": [
      {
        "published_port": 5673,
        "target_port": 5673,
        "protocol": "tcp",
        "publish_mode": "host"
      }
    ]
  },
  "networks": [
    {"target": "millegrille_net", "aliases": ["mq"]},
    {"target": "bridge"}
  ],
  "labels": {
    "millegrille": "${IDMG}"
  },
  "resources": {
    "cpu_limit": 3000000000,
    "mem_limit": 2100000000
  },
  "restart_policy": {
    "condition": "any",
    "delay": 5000000000,
    "max_attempts": 1000
  },
  "mode": {
    "mode": "replicated",
    "replicas": 1
  }
}
