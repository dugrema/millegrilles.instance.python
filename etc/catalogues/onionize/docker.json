{
  "nom": "onionize",
  "version": "2022.8.0",
  "securite": "1.public",
  "dependances": [
    {
      "name": "onionize",
      "image": "docker.maceroc.com/onionize:release-0.3_0",
      "config": {
        "name": "onionize",
        "mounts": [
          {"source": "/var/run/docker.sock", "target": "/tmp/docker.sock", "type": "bind", "read_only": true},
          {"source": "onionize-config", "target": "/var/lib/tor", "type": "volume"}
        ],
        "networks": [{"target": "millegrille_net", "aliases": ["onionize"]}],
        "labels": {"application": "onionize"},
        "container_labels": {"onionize": "true"},
        "resources": {
          "cpu_limit": 500000000,
          "mem_limit": 73741824
        },
        "restart_policy": {
          "condition": "on-failure",
          "delay": 60000000000,
          "max_attempts": 1
        },
        "mode": {
          "mode": "replicated",
          "replicas": 1
        }
      }
    }
  ]
}